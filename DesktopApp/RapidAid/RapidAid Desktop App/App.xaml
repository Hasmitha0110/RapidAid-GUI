<Application x:Class="RapidAid_Desktop_App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RapidAid_Desktop_App"
             xmlns:converters="clr-namespace:RapidAid_Desktop_App.Converters"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <converters:StatusColorConverter x:Key="StatusColorConverter"/>
            <converters:AdminCommentConverter x:Key="AdminCommentConverter"/>
            <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
            <converters:BooleanToTextConverter x:Key="BooleanToTextConverter"/>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
